/**
 * SnowPlow event object definition
 *
 * This is being called "Event2" temporarily so that there is no conflict with the equivalent 
 * definition in `event.avsc`
 */

@namespace("com.snowplowanalytics.snowplow.storage.avro")
protocol Event {
	import idl "snowplow_version/SnowPlowVersion.avdl";
	import idl "timestamp/Timestamp.avdl";

	record Event {
		string event_id;
		Timestamp timestamp;
		SnowPlowVersion snowplow_version;
	}
}