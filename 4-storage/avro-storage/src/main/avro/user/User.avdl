/**
 * SnowPlow user fields common across platforms
 * and event types
 */

@namespace("com.snowplowanalytics.snowplow.storage.avro")
protocol UserProtocol {
	record StringOrNull {
		union {string, null} StringOrNull;
	}

	record User {
		StringOrNull business_user_id;
		StringOrNull canonical_user_id;
		string ip_address;
	}
}