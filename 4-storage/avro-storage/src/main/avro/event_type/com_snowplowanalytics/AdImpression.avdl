/**
 * AdImpression event_type
 */

@namespace("com.snowplowanalytics.snowplow.storage.avro")
protocol AdImpressionProtocol {
	import idl "../../simple_types/Properties.avdl";

	record AdImpression {
		string banner_id;
		string campaign_id;
		string advertiser_id;
		string user_id;
		string zone_id;
		string publisher_id;
		map<Properties> custom_variables;
	}
}